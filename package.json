{
  "name": "figi-sdk",
  "version": "1.0.0",
  "description": "Unofficial TypeScript SDK for OpenFIGI API - Map financial identifiers to FIGIs (community-maintained)",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "bun run build && bun run test && bunx changeset publish",
    "prepublishOnly": "bun run build && bun run test && bun run lint && bun run typecheck",
    "pre-publish": "bun scripts/pre-publish.ts"
  },
  "keywords": [
    "openfigi",
    "figi",
    "finance",
    "api",
    "sdk",
    "typescript",
    "financial-identifiers",
    "bloomberg"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viktorlarsson/openfigi.git"
  },
  "bugs": {
    "url": "https://github.com/viktorlarsson/openfigi/issues"
  },
  "homepage": "https://github.com/viktorlarsson/openfigi#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@changesets/cli": "^2.29.8",
    "@types/bun": "^1.3.6",
    "@types/node": "^25.0.9",
    "@vitest/ui": "^4.0.17",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.16",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "ky": "^1.14.2",
    "zod": "^4.3.5"
  }
}
